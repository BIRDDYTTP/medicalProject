<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #FFF8DC;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SearchController">
   <children>
      <Label layoutX="345.0" layoutY="46.0" text="Search" textFill="#d62424">
         <font>
            <Font name="Paytone One" size="32.0" />
         </font>
      </Label>
      <TextField fx:id="input" layoutX="307.0" layoutY="110.0" promptText="input id" />
      <TableView fx:id="tableView" layoutX="36.0" layoutY="153.0" prefHeight="389.0" prefWidth="730.0">
        <columns>
          <TableColumn fx:id="h_id" prefWidth="101.0" text="h_id" />
          <TableColumn fx:id="date" prefWidth="147.0" text="date" />
            <TableColumn fx:id="detail" prefWidth="481.0" text="detail" />
        </columns>
      </TableView>
      <Button fx:id="search" layoutX="514.0" layoutY="110.0" mnemonicParsing="false" onAction="#onClickSearchBtn" text="search" />
      <Button fx:id="back" layoutX="36.0" layoutY="555.0" mnemonicParsing="false" onAction="#onClickBackBtn" text="back" />
   </children>
</AnchorPane>
